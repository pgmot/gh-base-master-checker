{
  "manifest_version": 2,

  "name": "Github Base Master Checker",
  "description": "Github Base Master Checker",
  "version": "1.0",

  "content_scripts": [
      {
          "matches": ["https://github.com/*"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],

  "permissions": [
    "https://github.com/"
  ]
}